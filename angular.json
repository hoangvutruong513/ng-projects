{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "schematics": {
    "@schematics/angular:application": {
      "style": "css",
      "inlineStyle": true
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "style": "css",
      "inlineStyle": true
    }
  },
  "projects": {
    "route-reuse-strat": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/route-reuse-strat",
      "sourceRoot": "projects/route-reuse-strat/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/route-reuse-strat/src/main.ts",
            "tsConfig": "projects/route-reuse-strat/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/route-reuse-strat/public"
              }
            ],
            "styles": ["projects/route-reuse-strat/src/styles.css"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "route-reuse-strat:build:production"
            },
            "development": {
              "buildTarget": "route-reuse-strat:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/route-reuse-strat/tsconfig.spec.json",
            "providersFile": "test-providers.ts",
            "setupFiles": ["test-setup.ts"],
            "runnerConfig": "vitest.config.ts",
            "reporters": ["html", "default"],
            "coverageReporters": ["text-summary", "html"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/route-reuse-strat/**/*.ts",
              "projects/route-reuse-strat/**/*.html"
            ],
            "eslintConfig": "projects/route-reuse-strat/eslint.config.mjs"
          }
        }
      }
    },
    "test-ssr": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/test-ssr",
      "sourceRoot": "projects/test-ssr/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/test-ssr/src/main.ts",
            "tsConfig": "projects/test-ssr/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/test-ssr/public"
              }
            ],
            "styles": ["projects/test-ssr/src/styles.css"],
            "server": "projects/test-ssr/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/test-ssr/src/server-hono.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "test-ssr:build:production"
            },
            "development": {
              "buildTarget": "test-ssr:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/test-ssr/tsconfig.spec.json",
            "providersFile": "test-providers.ts",
            "setupFiles": ["test-setup.ts"],
            "runnerConfig": "vitest.config.ts",
            "reporters": ["html", "default"],
            "coverageReporters": ["text-summary", "html"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/test-ssr/**/*.ts",
              "projects/test-ssr/**/*.html"
            ],
            "eslintConfig": "projects/test-ssr/eslint.config.mjs"
          }
        }
      }
    },
    "components": {
      "projectType": "library",
      "root": "projects/components",
      "sourceRoot": "projects/components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/components/tsconfig.spec.json",
            "providersFile": "test-providers.ts",
            "setupFiles": ["test-setup.ts"],
            "runnerConfig": "vitest.config.ts",
            "reporters": ["html", "default"],
            "coverageReporters": ["text-summary", "html"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/components/**/*.ts",
              "projects/components/**/*.html"
            ],
            "eslintConfig": "projects/components/eslint.config.mjs"
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["angular-eslint"],
    "analytics": false
  }
}
